@font-face {
  font-family 'FontAwesome'
  src url(_fa_src + 'eot?#iefix&v=' + _fa_v) format('embedded-opentype'),
  url(_fa_src + 'woff2?v=' + _fa_v) format('woff2'),
  url(_fa_src + 'woff?v=' + _fa_v) format('woff'),
  url(_fa_src + 'ttf?v=' + _fa_v) format('truetype'),
  url(_fa_src + 'svg?v=' + _fa_v + '#fontawesomeregular') format('svg')
  font-weight normal
  font-style normal
}
.fa
  display inline-block
  font normal normal normal 14px/1.45 FontAwesome
  font-size inherit
  text-rendering auto
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

